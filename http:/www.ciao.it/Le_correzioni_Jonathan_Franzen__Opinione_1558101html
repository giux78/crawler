<script language="JavaScript">
	function ReviewPageDisplayTooltip( elem ){
		document.getElementById( 'evaluate_tooltip_' + elem ).style.display = 'block';
	}

	function ReviewPageHideTooltip( elem ){
		document.getElementById( 'evaluate_tooltip_' + elem ).style.display = 'none';
	}

	function ReviewPageEvaluate( elem ){
		document.getElementById( 'rating_field' ).value = elem;
		document.getElementById( 'evaluate_review_form' ).submit();
	}
