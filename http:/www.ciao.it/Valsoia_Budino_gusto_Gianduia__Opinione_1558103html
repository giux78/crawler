function tabSwitch( oTab ) {
	aLIs = oTab.parentNode.getElementsByTagName( 'LI' );

	for( i=0; i < aLIs.length; i++ ) {
		aLIs[i].className = '';
	}

	oTab.className = 'active';

	// searching for container
	while( oTab.className != 'CWContainerBody' ) {
		oTab = oTab.parentNode;
	}

	aDivs = oTab.getElementsByTagName( 'DIV' );

	iCounter = 0;
	for( i=0; i < aDivs.length; i++ ) {
		// check if current div has tab content
		if( aDivs[i].className == 'MoreReviewsBox' ) {
			if( aLIs[iCounter].className == 'active' )
				aDivs[i].style.display = '';
			else
				aDivs[i].style.display = 'none';
			iCounter++;
		}
	}

}
