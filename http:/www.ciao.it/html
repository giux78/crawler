}</style><script type="text/javascript">
// <![CDATA[
	$(document).ready(function(){
		setTimeout( function(){
			$(document).bind( 'SLI_LoggedIn', {}, function(){xajax_CiaoHomepage_afterLogin()} );
			var so = new SWFObject("/home_member_circle.swf","home_member_circle","493","217","6","#FFFFFF","opaque");
			so.addVariable("dataurl", "/home_member_circle_data.php");
			so.write("MemberTeaserFlash");
		},1500 );
		
		$( '.flip-thru' ).each( function() {
			var iStart = 0;
			var iMaxItems = parseInt( $( this ).attr( 'rel' ) ) > 0 ? parseInt( $( this ).attr( 'rel' ) ) : 1;
			var oBox = $( this );
			var _showArrows = function() {
				if ( iStart <= 0 ) {
					oBox.find( '.flip-prev' ).hide();
				} else {
					oBox.find( '.flip-prev' ).show();
				}
				if ( oBox.find( '.flip-item' ).get( iStart + 1 ) ) {
					oBox.find( '.flip-next' ).show();
				} else {
					oBox.find( '.flip-next' ).hide();
				}
			}
			var _showItems = function() {
				oBox.find( '.flip-item' ).hide().removeClass( 'active' );
				for ( var iPosition = iStart; iPosition < ( iStart + iMaxItems ); iPosition++ ) {
					oBox.find( '.flip-item:eq(' + iPosition + ')' ).show()
						.find( '.flip-image' ).each( function() {
							$( this ).replaceWith( '<img src="' + $( this ).attr( 'rel' ) + '" style="' + $( this ).attr( 'style' ) + '" alt="' + $( this ).attr( 'title' )  + '" title="' + $( this ).attr( 'title' )  + '" />' );
						} );
				}
				if ( oBox.find( '.flip-view' ).get( iStart ) ) {
					oBox.find( '.flip-item:eq(' + iStart + ')' ).addClass( 'active' );
				} else {
					oBox.find( '.flip-item' ).removeClass( 'qaactive' ).end()
						.find( '.flip-item:eq(' + iStart + ')' ).addClass( 'qaactive' );
				}
				_showArrows();
			}
			oBox.find( '.flip-prev' ).unbind().click( function( e ) {
				iStart = iStart - 1;
				if ( oBox.find( '.flip-view' ).get( iStart ) ) {
					oBox.find( '.flip-item:eq(' + iStart + ')' ).trigger( 'click' );
				} else {
					_showItems();
				}
				return false;
			} );
			oBox.find( '.flip-next' ).unbind().click( function( e ) {
				iStart = iStart + 1;
				if ( oBox.find( '.flip-view' ).get( iStart ) ) {
					oBox.find( '.flip-item:eq(' + iStart + ')' ).trigger( 'click' );
				} else {
					_showItems();
				}
				return false;
			} );
			oBox.find( '.flip-item' ).each( function( iItemIndex ) {
				var oView = oBox.find( '.flip-view' ).get( iItemIndex );
				if ( oView ) {
					$( this ).unbind().click( function( e ) {
						if ( $( this ).hasClass( 'active' )  ) {
							return false;
						}
						oBox.find( '.flip-view' ).hide().removeClass( 'qaactive' );
						$( oView ).show().addClass( 'qaactive' )
							.find( '.flip-image' ).each( function() {
								$( this ).replaceWith( '<img src="' + $( this ).attr( 'rel' ) + '" style="' + $( this ).attr( 'style' ) + '" alt="' + $( this ).attr( 'title' )  + '" title="' + $( this ).attr( 'title' )  + '" />' );
							} );
						iStart = iItemIndex;
						_showItems();
						return false;
					} );
				}
			} );
			_showItems();
		} );

		// focus the main search input
		$( '#HEMAIN_search_string' ).focus();
	});
// ]]>
</script>    </head>
