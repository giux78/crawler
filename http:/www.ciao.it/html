<script type="text/javascript">
//<![CDATA[
var bExelatorCompleted = false;
$(function () {
	var _write = document.write;
	document.write = function( htmlContents ) {
		var cont = document.createElement("div");
		cont.className = "dynamically-added";
		cont.innerHTML = htmlContents;
		var mainBody = document.getElementsByTagName("body")[0];
		mainBody.appendChild(cont);
	}
	var exelatorScript = document.createElement("script");
	exelatorScript.type = "text/javascript";
	exelatorScript.async = true;
	exelatorScript.src = document.location.protocol + "//loadeu.exelator.com/load/?p=446&c=20210&g=004&ctg=Shopping&subctg=&count=&lang=&kw=";
	exelatorScript.onload = exelatorScript.onreadystatechange = function() { 
		if ( !bExelatorCompleted && ( !this.readyState || this.readyState === "loaded" || this.readyState === "complete" ) ) {
				bExelatorCompleted = true;
				document.write = _write; 
				exelatorScript.onload = exelatorScript.onreadystatechange = null;
		}
	}
	var bodyTag = document.getElementsByTagName("body")[0];
	bodyTag.appendChild(exelatorScript);
});
//]]>
</script>
